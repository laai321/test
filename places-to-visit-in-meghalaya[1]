    <div class="base-block-image onclick-link" data-href="">
      <picture class="lazy-picture"><source data-srcset="https://images.thrillophilia.com/image/upload/s--kNqBLiKK--/c_fill,g_center,h_450,q_auto,w_753/dpr_1.0,f_auto,fl_strip_profile/v1/images/photos/000/130/947/original/1533199303_1200px-Entrance_to_Air_Force_Museum_2C_Palam_2C_New_Delhi.JPG.jpg 1x, https://images.thrillophilia.com/image/upload/s--SqCBch47--/c_fill,g_center,h_450,q_auto,w_753/dpr_1.5,f_auto,fl_strip_profile/v1/images/photos/000/130/947/original/1533199303_1200px-Entrance_to_Air_Force_Museum_2C_Palam_2C_New_Delhi.JPG.jpg 2x" media="(min-width: 1280px)"></source><source data-srcset="https://images.thrillophilia.com/image/upload/s--kNqBLiKK--/c_fill,g_center,h_450,q_auto,w_753/dpr_1.0,f_auto,fl_strip_profile/v1/images/photos/000/130/947/original/1533199303_1200px-Entrance_to_Air_Force_Museum_2C_Palam_2C_New_Delhi.JPG.jpg 1x, https://images.thrillophilia.com/image/upload/s--SqCBch47--/c_fill,g_center,h_450,q_auto,w_753/dpr_1.5,f_auto,fl_strip_profile/v1/images/photos/000/130/947/original/1533199303_1200px-Entrance_to_Air_Force_Museum_2C_Palam_2C_New_Delhi.JPG.jpg 2x" media="(min-width: 1024px)"></source><source data-srcset="https://images.thrillophilia.com/image/upload/s--p3EfDJO3--/c_fill,g_center,h_460,q_auto,w_800/dpr_1.0,f_auto,fl_strip_profile/v1/images/photos/000/130/947/original/1533199303_1200px-Entrance_to_Air_Force_Museum_2C_Palam_2C_New_Delhi.JPG.jpg 1x, https://images.thrillophilia.com/image/upload/s--s6vaDSxd--/c_fill,g_center,h_460,q_auto,w_800/dpr_1.5,f_auto,fl_strip_profile/v1/images/photos/000/130/947/original/1533199303_1200px-Entrance_to_Air_Force_Museum_2C_Palam_2C_New_Delhi.JPG.jpg 2x" media="(min-width: 768px)"></source><source data-srcset="https://images.thrillophilia.com/image/upload/s--DFVaCMwo--/c_fill,g_center,h_460,q_auto,w_768/dpr_1.0,f_auto,fl_strip_profile/v1/images/photos/000/130/947/original/1533199303_1200px-Entrance_to_Air_Force_Museum_2C_Palam_2C_New_Delhi.JPG.jpg 1x, https://images.thrillophilia.com/image/upload/s-